{
  "manifest_version": 2,

  "name": "URL Expander",
  "description": "This extension expands Sina Weibo short URL.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "expander.html"
  },
  "permissions": [
    "http://api.t.sina.com.cn/",
    "tabs"
  ]
}
